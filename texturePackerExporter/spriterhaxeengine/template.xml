<TextureAtlas imagePath="{{texture.fullName}}">{% for sprite in allSprites %}
	<Sub name="{{sprite.fullName}}" x="{{sprite.frameRect.x}}" y="{{sprite.frameRect.y}}" w="{{sprite.frameRect.width}}" h="{{sprite.frameRect.height}}" {% if sprite.trimmed %} frameX="{{sprite.cornerOffset.x}}" frameY="{{sprite.cornerOffset.y}}" frameW="{{sprite.untrimmedSize.width}}" frameH="{{sprite.untrimmedSize.height}}"{% endif %}/>{% endfor %}
</TextureAtlas>